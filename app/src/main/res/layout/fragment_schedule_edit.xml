        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeEdit2"
        app:layout_constraintVertical_bias="0.74" />

    <ImageView
