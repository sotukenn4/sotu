        app:layout_constraintTop_toBottomOf="@+id/timeEdit"
        app:layout_constraintVertical_bias="0.487" />

    <View
        android:id="@+id/view8"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="@android:color/black"
