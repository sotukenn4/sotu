        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666" />

    <ImageView
        android:id="@+id/dateframe"
        android:layout_width="176dp"
        android:layout_height="41dp"
        android:src="@drawable/frame_style"
        app:layout_constraintBottom_toBottomOf="@+id/dateEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.548"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/dateEdit"
        app:layout_constraintVertical_bias="0.428" />

    <EditText
        android:id="@+id/dateEdit"
        android:layout_width="167dp"
        android:layout_height="34dp"
        android:layout_marginTop="28dp"
        android:background="@color/white"
        android:ems="10"
        android:hint="@string/hint"
        android:inputType="date"
        android:shadowColor="@color/black"
        android:textAlignment="viewStart"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleEdit" />

    <ImageView
        android:id="@+id/timeframe1"
        android:layout_width="176dp"
        android:layout_height="42dp"
        android:src="@drawable/frame_style"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/timeEdit" />

    <EditText
        android:id="@+id/timeEdit"
        android:layout_width="167dp"
        android:layout_height="35dp"
        android:layout_marginTop="24dp"
        android:background="@color/white"
        android:ems="10"
        android:hint="@string/hint2"
        android:inputType="time"
        android:shadowColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dateEdit" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="176dp"
        android:layout_height="42dp"
        android:src="@drawable/frame_style"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/timeEdit2"
        app:layout_constraintVertical_bias="0.571" />

    <EditText
        android:id="@+id/timeEdit2"
        android:layout_width="167dp"
        android:layout_height="35dp"
        android:layout_marginTop="36dp"
        android:background="@color/white"
        android:ems="10"
        android:hint="@string/hint2"
        android:inputType="time"
        android:shadowColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeEdit" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="351dp"
        android:layout_height="89dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/frame_style"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/detailEdit"
        android:layout_width="331dp"
        android:layout_height="73dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="start|top"
        android:hint="　詳細１"
        android:inputType="textMultiLine"
        android:maxLines="8"
        android:minLines="2"
        android:shadowColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="43dp"
        android:layout_height="29dp"
        android:layout_marginStart="8dp"
        android:text="日付"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/dateEdit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/dateEdit"
        app:layout_constraintVertical_bias="0.529" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="85dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:text="開始時間"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/timeEdit"
        app:layout_constraintVertical_bias="0.529" />

    <TextView
        android:id="@+id/finishText"
        android:layout_width="84dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:text="終了時間"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/timeEdit2"
        app:layout_constraintVertical_bias="0.833" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="タイトル"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/titleEdit"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/titleEdit" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="45dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="詳細"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeEdit2" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="164dp"
        android:layout_height="26dp"
        android:spinnerMode="dialog"
        app:layout_constraintBottom_toBottomOf="@+id/titleEdit2"
        app:layout_constraintEnd_toEndOf="@+id/titleEdit2"
        app:layout_constraintStart_toStartOf="@+id/titleEdit2"
        app:layout_constraintTop_toTopOf="@+id/titleEdit2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/datebuttonframe"
        android:layout_width="54dp"
        android:layout_height="51dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/layout_box_3"
        app:layout_constraintBottom_toBottomOf="@+id/dateEdit"
        app:layout_constraintStart_toEndOf="@+id/dateframe"
        app:layout_constraintTop_toTopOf="@+id/dateframe"
        app:layout_constraintVertical_bias="0.666" />

    <Button
        android:id="@+id/datebutton"
        android:layout_width="38dp"
        android:layout_height="30dp"
        android:background="@drawable/ic_baseline_calendar_today_24"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/datebuttonframe"
        app:layout_constraintEnd_toEndOf="@+id/datebuttonframe"
        app:layout_constraintStart_toStartOf="@+id/datebuttonframe"
        app:layout_constraintTop_toTopOf="@+id/datebuttonframe" />

    <TextView
        android:id="@+id/datebuttonframe2"
        android:layout_width="54dp"
        android:layout_height="51dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/layout_box_3"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit"
        app:layout_constraintStart_toEndOf="@+id/timeEdit"
        app:layout_constraintTop_toTopOf="@+id/timeEdit"
        app:layout_constraintVertical_bias="0.312" />

    <TextView
        android:id="@+id/datebuttonframe3"
        android:layout_width="54dp"
        android:layout_height="51dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/layout_box_3"
        app:layout_constraintBottom_toBottomOf="@+id/timeEdit2"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/timeEdit2" />

    <Button
        android:id="@+id/timeButton"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/ic_baseline_access_time_24"
        app:backgroundTint="@color/design_default_color_surface"
        app:layout_constraintBottom_toBottomOf="@+id/datebuttonframe2"
        app:layout_constraintEnd_toEndOf="@+id/datebuttonframe2"
        app:layout_constraintStart_toStartOf="@+id/datebuttonframe2"
        app:layout_constraintTop_toTopOf="@+id/datebuttonframe2"
        app:layout_constraintVertical_bias="0.428" />

    <Button
        android:id="@+id/timeButton2"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/ic_baseline_access_time_24"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/datebuttonframe3"
        app:layout_constraintEnd_toEndOf="@+id/datebuttonframe3"
        app:layout_constraintStart_toStartOf="@+id/datebuttonframe3"
        app:layout_constraintTop_toTopOf="@+id/datebuttonframe3"
        app:layout_constraintVertical_bias="0.538" /><![CDATA[


























    android:text="Button"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="239dp" />













]]>
</androidx.constraintlayout.widget.ConstraintLayout>